{
  "name": "webpack-setup",
  "version": "1.0.0",
  "description": "My standard webpack setup",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --open --env.development",
    "dev": "cross-env NODE_ENV=development webpack -d --env.development",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --env.production",
    "serve": "npm start",
    "development": "npm run dev",
    "production": "npm run build"
  },
  "author": "thegingerdk <iam@theginger.dk>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-inline-svg-plugin": "^0.2.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "imagemin-webpack-plugin": "^1.5.2",
    "jimp": "^0.2.28",
    "node-sass": "^4.7.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "prerender-spa-plugin": "^2.1.0",
    "responsive-loader": "^1.1.0",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "sharp": "^0.18.4",
    "style-loader": "^0.19.1",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-svg-loader": "^0.4.0",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
